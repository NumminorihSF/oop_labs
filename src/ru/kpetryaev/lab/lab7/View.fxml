<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<fx:root fx:id="root" hgap="10.0" layoutX="1.0" layoutY="1.0" type="javafx.scene.layout.GridPane" vgap="10.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" >

        <columnConstraints>
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="NEVER" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
        <children>
            <MenuBar prefHeight="25.0" prefWidth="289.0" GridPane.columnSpan="2" GridPane.valignment="TOP">
                <menus>
                    <Menu mnemonicParsing="false" text="Линейные алгоритмы">
                        <items>
                            <MenuItem fx:id="task1" mnemonicParsing="false" text="Задача №1" />
                            <MenuItem fx:id="task2" mnemonicParsing="false" text="Задача №2" />
                        </items>
                    </Menu>
                    <Menu mnemonicParsing="false" text="Ветвление">
                        <items>
                            <MenuItem fx:id="task3" mnemonicParsing="false" text="Задача №3" />
                            <MenuItem fx:id="task4" mnemonicParsing="false" text="Задача №4" />
                        </items>
                    </Menu>
                    <Menu fx:id="exit" mnemonicParsing="false" text="Выход" >
                    <items>
                        <MenuItem mnemonicParsing="false" text="Never seen" />
                    </items>
                    </Menu>
                </menus>
            </MenuBar>
        </children>

</fx:root>
